{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "strings.schema.json",
    "type": "object",
    "patternProperties": {
        "^.*$": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            ]
        }
    }
}